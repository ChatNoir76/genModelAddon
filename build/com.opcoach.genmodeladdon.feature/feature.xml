<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.opcoach.genmodeladdon.feature"
      label="EMF genModelAddon (override generated code)"
      version="2.2.0.qualifier"
      provider-name="OPCOACH">

   <description url="http://www.example.com/description">
      This feature is used to generate a &apos;src&apos; directory extending
the EMF generated directory.

The home page of this project is here : http://opcoach.github.io/genModelAddon/
   </description>

   <copyright>
      @ 2017 OPCoach
   </copyright>

   <license url="http://www.example.com/license">
      This feature is distributed under EPL License.

Thank you to star this project on its github page : 
https://github.com/opcoach/genModelAddon 

Get the home page of this project here : 
http://opcoach.github.io/genModelAddon/
   </license>

   <requires>
      <import plugin="org.eclipse.emf.codegen.ecore"/>
      <import plugin="org.eclipse.xtext.xbase.lib" version="2.7.3" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.pde.core"/>
      <import plugin="org.apache.commons.io"/>
      <import plugin="org.eclipse.jdt.core" version="3.10.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ant.core" version="3.4.0" match="greaterOrEqual"/>
      <import plugin="com.opcoach.genmodeladdon.core" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources" version="3.9.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.e4.ui.services" version="1.1.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jface" version="3.10.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.e4.core.di" version="1.4.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.xtend"/>
      <import plugin="com.google.guava" version="14.0.0"/>
   </requires>

   <plugin
         id="com.opcoach.genmodeladdon.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.opcoach.genmodeladdon.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.xtext.xbase.lib"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
