<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.opcoach.genmodeladdon.feature"
      label="Emf override generated code feature"
      version="1.0.0.qualifier"
      provider-name="OPCOACH">

   <description url="http://www.example.com/description">
      This feature is used to generate a &apos;src&apos; directory extending the EMF generated directory.
   </description>

   <copyright url="http://www.example.com/copyright">
      @ 2015 OPCoach
   </copyright>

   <license url="http://www.example.com/license">
      This feature is distributed under EPL License
   </license>

   <requires>
      <import plugin="org.eclipse.emf.codegen.ecore"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.pde.core"/>
      <import plugin="org.eclipse.pde.ui"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="com.opcoach.genmodeladdon.core" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.expressions"/>
      <import plugin="com.google.guava" version="10.0.1"/>
   </requires>

   <plugin
         id="com.opcoach.genmodeladdon.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.opcoach.genmodeladdon.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.xtext.xbase.lib"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
